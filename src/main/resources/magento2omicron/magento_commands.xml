<!-- Definition for CLI commands. Usage in com.magento2utilities.packages.models.Commands.TerminalExecution -->
<commands>
    <command id="cache_flush"     execution="magento cache:flush"                title="Flush Cache"/>
    <command id="setup_upgrade"   execution="magento setup:upgrade"              title="Setup Upgrade"/>
    <command id="developer_mode"  execution="magento deploy:mode:set developer"  title="Enable Developer Mode"/>
    <command id="production_mode" execution="magento deploy:mode:set production" title="Enable Production Mode"/>
    <command id="reindex_full"    execution="magento indexer:reindex"            title="Reindex System"/>
    <command id="reindex_reset"   execution="magento indexer:reset"              title="Reset Indexers"/>
    <command id="di_compile"      execution="magento setup:di:compile"           title="DI Compile"/>
</commands>